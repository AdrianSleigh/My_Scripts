UNION ALL SELECT N'🖨️' AS Icon, N'Print' AS Icon_Name, N'Used to print documents' AS Description
UNION ALL SELECT N'🔗' AS Icon, N'Link' AS Icon_Name, N'Used to connect or reference' AS Description
UNION ALL SELECT N'🚫' AS Icon, N'Block' AS Icon_Name, N'Used to deny or restrict actions' AS Description
UNION ALL SELECT N'✅' AS Icon, N'Confirm' AS Icon_Name, N'Used to approve or validate actions' AS Description
UNION ALL SELECT N'➕' AS Icon, N'Add' AS Icon_Name, N'Used to insert new items' AS Description
UNION ALL SELECT N'➖' AS Icon, N'Remove' AS Icon_Name, N'Used to subtract or delete' AS Description
UNION ALL SELECT N'📁' AS Icon, N'Folder' AS Icon_Name, N'Used to represent a directory' AS Description
UNION ALL SELECT N'📂' AS Icon, N'Open Folder' AS Icon_Name, N'Used to open a directory' AS Description
UNION ALL SELECT N'📄' AS Icon, N'Document' AS Icon_Name, N'Used to represent a file' AS Description
UNION ALL SELECT N'🔙' AS Icon, N'Back' AS Icon_Name, N'Used to navigate to the previous page' AS Description
UNION ALL SELECT N'🔜' AS Icon, N'Forward' AS Icon_Name, N'Used to navigate to the next page' AS Description
