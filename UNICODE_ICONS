UNION ALL SELECT N'🔽' AS Icon, N'Down' AS Icon_Name, N'Used to move downward' AS Description
UNION ALL SELECT N'🔧' AS Icon, N'Tool' AS Icon_Name, N'Used to access utilities or tools' AS Description
